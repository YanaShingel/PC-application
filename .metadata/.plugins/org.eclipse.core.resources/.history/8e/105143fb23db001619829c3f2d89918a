		// TODO:
	}
	
	public void setXAxisRange(int min, int max){
		// TODO:
	}
	
	public GraphBuilder(int x0, int y0, int width, int height, JPanel parent){
		
		g = parent.getGraphics();
		g.setClip(x0, y0, x0 + width, y0 + height);
		g.fillRect(x0, y0, width, height);
		
	}

}
